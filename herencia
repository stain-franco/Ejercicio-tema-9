public class Datos {
    public static void main (String[]args){
        Cliente cliente = new Cliente();
        cliente.nombre = "arturo";
        cliente.edad = 28;
        cliente.telefono = 322683591;
        cliente.credito = 12345;


        Trabajador trabajador = new Trabajador();
        trabajador.nombre = "camilo";
        trabajador.edad = 29;
        trabajador.telefono = 38616496;
        trabajador.salario = 1.357;

        System.out.println("su nombre es: "+cliente.nombre);
        System.out.println("su edad es: "+cliente.edad);
        System.out.println("su telefono es: "+cliente.telefono);
        System.out.println("su credito es de: "+cliente.credito);
        System.out.println("\nsu nombre es: "+trabajador.nombre);
        System.out.println("su edad es: "+trabajador.edad);
        System.out.println("su telefono es: "+trabajador.telefono);
        System.out.println("su salario es de: "+trabajador.salario);
    }
}
class Persona{
    int edad;
    String nombre;
    int telefono;

}
class Cliente extends Persona{
    int credito;
}

class Trabajador extends Persona{
    double salario;
}
